    "babel-loader": "^8.1.0",
    "css-loader": "^3.4.2",
    "express": "^4.17.3",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.0.4",
    "image-webpack-loader": "^6.0.0",
